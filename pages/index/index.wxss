.index_swiper{
  width: 750rpx ;
}
.index_swiper image,.index_case image,.floor_title image,.floor_group image{
  width: 100%;
}
.index_case{
  display: flex;
}
.index_case navigator{
  flex:1;
  padding: 20rpx;
}
.index_floor{
  padding: 0 10rpx;
}
.floor_title {
  padding: 10rpx 0;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator{
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-last-child(-n+4){
  height: 27.72711206896552vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image {
  width: 100%;
  height: 100%;
}

